{
  "name": "@tinkubot/frontend",
  "version": "1.0.0",
  "description": "Frontend Service para TinkuBot - Node.js + Express + Bootstrap 5",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "dev:dashboard": "npm run dev --workspace @tinkubot/admin-dashboard",
    "build": "npm run build --workspace @tinkubot/admin-dashboard",
    "preview:dashboard": "npm run preview --workspace @tinkubot/admin-dashboard",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "quality-check": "npm run format:check && npm run lint && npm audit"
  },
  "dependencies": {
    "@eslint/js": "^9.36.0",
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^3.0.1",
    "patch-package": "^8.0.0",
    "prettier": "^3.1.0"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "frontend",
    "nodejs"
  ],
  "author": "TinkuBot",
  "license": "MIT"
}
